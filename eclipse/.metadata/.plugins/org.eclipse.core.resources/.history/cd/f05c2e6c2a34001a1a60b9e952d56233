// CHILD CLASS METHOD OVERIDES PARENT CLASS METHOD

class A{
	public void show() {
		System.out.println("In A");
	}	
}

class B extends A{
	public void show() {
		System.out.println("In B");
	}
}

public class OveridingDemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		B obj1 = new B();
		obj1.show();
	}

}
